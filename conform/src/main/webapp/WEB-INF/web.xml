<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
      <servlet-name>conform</servlet-name>
      <servlet-class>com.bitmechanic.barrister.conform.ConformServlet</servlet-class>
  </servlet>

  <servlet>
      <servlet-name>exit</servlet-name>
      <servlet-class>com.bitmechanic.barrister.conform.ExitServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>conform</servlet-name>
      <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
      <servlet-name>exit</servlet-name>
      <url-pattern>/exit</url-pattern>
  </servlet-mapping>

</web-app>
